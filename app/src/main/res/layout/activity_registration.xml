<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="#f2f2f2"
    android:scrollbars="vertical"
    tools:ignore="RtlHardcoded,UnusedNamespace,UnknownIdInLayout">

    <RelativeLayout
        android:id="@+id/topbarlay"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentTop="true"
        android:background="#59c1b2">

        <ImageButton
            android:id="@+id/backbut"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginBottom="12dp"
            android:layout_marginRight="11dp"
            android:layout_marginTop="12dp"
            android:background="@drawable/back" />
    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/topbarlay"
        android:scrollbars="vertical">

        <TextView
            android:id="@+id/usernametv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_marginRight="30dp"
            android:layout_marginTop="25dp"
            android:text="@string/username"
            android:textColor="@color/black"
            android:textSize="@dimen/text_size_medium" />

        <EditText
            android:id="@+id/usernameet"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/usernametv"
            android:layout_alignBottom="@+id/usernametv"
            android:layout_alignParentLeft="true"
            android:layout_marginLeft="15dp"
            android:layout_marginRight="30dp"
            android:layout_toLeftOf="@+id/usernametv"
            android:gravity="right"
            android:inputType="textPersonName"
            android:textColor="@color/black"
            android:textColorHint="@color/green"
            android:textSize="@dimen/text_size_medium">

            <requestFocus />
        </EditText>

        <View
            android:layout_width="fill_parent"
            android:layout_height="1dp"
            android:layout_alignLeft="@+id/usernameet"
            android:layout_alignRight="@+id/usernametv"
            android:layout_below="@+id/usernameet"
            android:layout_marginTop="10dp"
            android:background="#c0c0c0" />

        <TextView
            android:id="@+id/emailtv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_below="@+id/usernameet"
            android:layout_marginRight="30dp"
            android:layout_marginTop="50dp"
            android:text="@string/email"
            android:textColor="@color/black"
            android:textSize="@dimen/text_size_medium" />

        <EditText
            android:id="@+id/emailet"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/emailtv"
            android:layout_alignBottom="@+id/emailtv"
            android:layout_alignParentLeft="true"
            android:layout_alignRight="@+id/usernameet"
            android:layout_marginLeft="15dp"
            android:gravity="right"
            android:hint="@string/optional"
            android:inputType="textEmailAddress"
            android:textColor="@color/black"
            android:textColorHint="@color/green"
            android:textSize="@dimen/text_size_medium" />

        <View
            android:layout_width="fill_parent"
            android:layout_height="1dp"
            android:layout_alignLeft="@+id/usernameet"
            android:layout_alignRight="@+id/usernametv"
            android:layout_below="@+id/emailtv"
            android:layout_marginTop="15dp"
            android:background="#c0c0c0" />

        <TextView
            android:id="@+id/passwordtv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_below="@+id/emailet"
            android:layout_marginRight="30dp"
            android:layout_marginTop="25dp"
            android:text="@string/password"
            android:textColor="@color/black"
            android:textSize="@dimen/text_size_medium" />

        <android.support.design.widget.TextInputLayout
            android:id="@+id/passwordlay"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/usernameet"
            android:layout_alignRight="@+id/usernameet"
            android:layout_below="@id/emailet"
            android:layout_toLeftOf="@+id/passwordtv"
            app:passwordToggleEnabled="true">

            <android.support.design.widget.TextInputEditText
                android:id="@+id/passwordet"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="right"
                android:inputType="textPassword"
                android:textColor="@color/black"
                android:textSize="@dimen/text_size_medium" />

        </android.support.design.widget.TextInputLayout>


        <View
            android:id="@+id/view1"
            android:layout_width="fill_parent"
            android:layout_height="1dp"
            android:layout_alignLeft="@+id/usernameet"
            android:layout_alignRight="@+id/usernametv"
            android:layout_below="@+id/passwordlay"
            android:layout_marginTop="10dp"
            android:background="#c0c0c0" />

        <CheckBox
            android:id="@+id/agreeoncb"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignRight="@+id/countrytv"
            android:layout_below="@+id/countrytv"
            android:layout_marginTop="30dp"
            android:button="@drawable/checkbox"
            android:gravity="center_vertical|start" />


        <TextView
            android:id="@+id/agetv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignRight="@+id/view1"
            android:layout_below="@+id/view1"
            android:layout_marginTop="25dp"
            android:text="@string/dateofbirth"
            android:textColor="@color/black"

            android:textSize="@dimen/text_size_medium" />

        <TextView
            android:id="@+id/sextv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignRight="@+id/agetv"
            android:layout_below="@+id/agetv"
            android:layout_marginTop="25dp"
            android:text="@string/sex"
            android:textColor="@color/black"
            android:textSize="@dimen/text_size_medium" />


        <RadioGroup
            android:id="@+id/sexvaluerg"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/agetv"
            android:layout_marginLeft="45dp"

            android:layout_marginRight="20dp"
            android:layout_marginTop="25dp"

            android:layout_toLeftOf="@+id/agetv"
            android:gravity="right"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/femaletv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="5dp"
                android:gravity="center_vertical"
                android:text="@string/female"
                android:textColor="@color/black" />

            <RadioButton
                android:id="@+id/femalerb"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="10dp"
                android:button="@drawable/radiobutton"
                android:gravity="center_vertical"
                android:padding="2dp" />

            <TextView
                android:id="@+id/maletv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="5dp"
                android:gravity="center_vertical"
                android:text="@string/male"
                android:textColor="@color/black" />

            <RadioButton
                android:id="@+id/malerb"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:button="@drawable/radiobutton"
                android:checked="true"
                android:gravity="center_vertical"
                android:padding="2dp" />
        </RadioGroup>


        <TextView
            android:id="@+id/countrytv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignRight="@+id/agetv"
            android:layout_below="@+id/sextv"
            android:layout_marginTop="25dp"
            android:text="@string/country"
            android:textColor="@color/black"
            android:textSize="@dimen/text_size_medium" />

        <EditText
            android:id="@+id/agesp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/agetv"
            android:layout_alignLeft="@+id/usernameet"
            android:layout_alignRight="@+id/usernameet"
            android:layout_below="@+id/sextv"
            android:layout_marginTop="25dp"
            android:layout_toLeftOf="@+id/agetv"
            android:clickable="true"
            android:gravity="right"
            android:inputType="date"
            android:textColor="@color/black"
            android:textSize="@dimen/text_size_medium" />

        <RadioGroup
            android:id="@+id/agerg"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/agetv"
            android:layout_alignLeft="@+id/view1"
            android:layout_alignStart="@+id/view1"
            android:layout_marginLeft="13dp"
            android:layout_marginStart="13dp"
            android:orientation="horizontal" />

        <RadioGroup
            android:id="@+id/sexrg"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/sextv"
            android:orientation="horizontal" />

        <Spinner
            android:id="@+id/countrysp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/countrytv"
            android:layout_marginLeft="45dp"
            android:layout_marginRight="20dp"
            android:layout_toLeftOf="@+id/agetv"
            android:dropDownWidth="wrap_content"
            android:gravity="right"
            android:popupBackground="@color/white"
            android:textSize="@dimen/text_size_medium" />


        <TextView
            android:id="@+id/textView3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/agreeoncb"
            android:layout_alignBottom="@+id/agreeoncb"
            android:layout_toLeftOf="@+id/countrytv"
            android:text="@string/agreeon"
            android:textColor="#000000"
            android:textSize="@dimen/text_size_small" />

        <TextView
            android:id="@+id/privecytv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/textView3"
            android:layout_marginRight="2dp"
            android:layout_toLeftOf="@+id/textView3"
            android:background="@null"
            android:clickable="true"
            android:text="@string/privecyplicy"
            android:textColor="#59c1b2"
            android:textSize="@dimen/text_size_small" />

        <Button
            android:id="@+id/registerbut"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/privecytv"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="17dp"
            android:background="@drawable/button"
            android:text="@string/register"
            android:textColor="#ffffff"
            android:textSize="@dimen/text_size_large" />
    </RelativeLayout>


</RelativeLayout>